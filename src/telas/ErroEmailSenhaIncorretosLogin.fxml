<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerErroEmailSenhaIncorretosLogin">
   <children>
      <Pane prefHeight="150.0" prefWidth="380.0">
         <children>
            <ImageView accessibleText="Imagem com um ponto de exclamação, símbolo para informar erro." fitHeight="37.0" fitWidth="37.0" layoutX="28.0" layoutY="34.0">
               <image>
                  <Image url="@../imagens/exclamation-button.png" />
               </image>
            </ImageView>
            <Text accessibleText="O campo de email ou senha estão incorretos. Por favor, volte e preencha corretamente  ou vá para a tela de recuperação de senha." focusTraversable="true" layoutX="82.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Seu email ou sua senha estão incorretos">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Button fx:id="btnVoltarTelaLogin" accessibleText="Clique aqui para voltar para a Tela de Login e preencher novamente os campos." layoutX="28.0" layoutY="97.0" minHeight="25.0" mnemonicParsing="false" onAction="#VoltarTelaLogin" onKeyPressed="#buttonPressed" prefHeight="27.0" prefWidth="143.0" style="-fx-background-color: #b93026;" text="Preencher Login" textFill="WHITE" />
            <Button fx:id="btnRecuperarSenha" accessibleText="Clique aqui para ir até a Tela de Recuperação de senha." layoutX="206.0" layoutY="97.0" minHeight="25.0" mnemonicParsing="false" onAction="#telaRecuperarSenha" onKeyPressed="#buttonPressed2" prefHeight="27.0" prefWidth="143.0" style="-fx-background-color: #b93026;" text="Recuperar Minha Senha" textFill="WHITE" />
         </children>
      </Pane>
   </children>
</AnchorPane>
